{
    "name": "macghriogair/php-support",
    "description": "Collection of utils and helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Mac Gregor",
            "email": "macgregor.porta@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.4",
        "ext-mbstring": "*",
        "paragonie/random_compat": "~1.4|~2.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.4",
        "phpunit/phpunit": "~5.4"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Macghriogair\\Support\\": "src/"
        },
        "autoload-dev": {
          "classmap": ["tests/"]
        }
    }
}
