{
    "name": "macghriogair/php-support",
    "description": "Collection of utils and helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Mac Gregor",
            "email": "macgregor.porta@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "ext-mbstring": "*",
        "paragonie/random_compat": "~1.4|~2.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.4",
        "phpunit/phpunit": "6.*",
        "squizlabs/php_codesniffer": "2.*",
        "phploc/phploc": "dev-master",
        "sebastian/phpcpd": "2.0.*@dev",
        "pdepend/pdepend": "2.2.2",
        "phpmd/phpmd": "@stable"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Macghriogair\\Support\\": "src/"
        },
        "autoload-dev": {
          "classmap": ["tests/"]
        }
    }
}
